{
  "name": "garmin-connect-client",
  "version": "0.1.8",
  "description": "TypeScript library for interacting with Garmin Connect",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "garmin",
    "garmin-connect",
    "garmin-connect-client",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orpjones/garmin-connect-client.git"
  },
  "homepage": "https://github.com/orpjones/garmin-connect-client#readme",
  "bugs": {
    "url": "https://github.com/orpjones/garmin-connect-client/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/ui": "^1.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^50.0.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@types/qs": "^6.14.0",
    "axios": "^1.13.2",
    "axios-cookiejar-support": "^4.0.7",
    "form-data": "^4.0.5",
    "oauth-1.0a": "^2.2.6",
    "qs": "^6.14.0",
    "tough-cookie": "^4.1.3",
    "zod": "^3.22.4"
  }
}
